{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 28,
        "patch": 0
    },
    "configurePresets": [
        {
            "hidden": true,
            "name": "godot",
            "cacheVariables": {
                "GODOT_EXECUTABLE": "C:\\godot\\src\\godot\\bin\\godot.windows.editor.x86_64.exe",
                "GODOT_GDEXTENSION_DIR": "${sourceDir}/gdextension"
            }
        },
        {
            "hidden": true,
            "name": "flatbuffers",
            "cacheVariables": {
                "FLATBUFFERS_BUILD_FLATHASH": false,
                "FLATBUFFERS_BUILD_SHAREDLIB": false,
                "FLATBUFFERS_BUILD_TESTS": false,
                "FLATBUFFERS_INSTALL": false,
                "FLATBUFFERS_BUILD_FLATC": false
            }
        },
        {
            "hidden": true,
            "name": "godot-cpp",
            "cacheVariables": {
                "GODOTCPP_GIT_URL": "https://github.com/enetheru/godot-cpp.git",
                "GODOTCPP_GIT_TAG": "4.3-modernise"
            }
        }
    ]
}
